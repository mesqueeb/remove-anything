{
  "name": "remove-anything",
  "sideEffects": false,
  "version": "0.0.2",
  "description": "Remove any prop (eg. `undefined`) from an object",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.esm.js",
      "types": "./types/index.d.ts"
    }
  },
  "scripts": {
    "test": "ava",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build": "rollup -c build.js",
    "release": "npm run lint && npm run test && npm run build && np"
  },
  "dependencies": {
    "is-what": "^3.14.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "ava": "^4.0.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-tree-shaking": "^1.10.0",
    "np": "^7.6.0",
    "prettier": "^2.5.1",
    "rollup": "^2.63.0",
    "rollup-plugin-typescript2": "^0.31.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "keywords": [
    "remove",
    "remove-prop",
    "remove-anything",
    "remove-undefined"
  ],
  "author": "Luca Ban - Mesqueeb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mesqueeb/remove-anything/issues"
  },
  "homepage": "https://github.com/mesqueeb/remove-anything#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mesqueeb/remove-anything.git"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "tsconfig-paths/register",
      "ts-node/register"
    ]
  },
  "np": {
    "yarn": false,
    "branch": "production"
  }
}
